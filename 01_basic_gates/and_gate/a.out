#! /usr/local/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
:vpi_module "/usr/local/lib/ivl/v2009.vpi";
S_0x2af8640 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x2ad4020 .scope module, "and_gate_test" "and_gate_test" 3 2;
 .timescale 0 0;
v0x2b1e0a0_0 .var "a", 0 0;
v0x2b1e170_0 .var "b", 0 0;
v0x2b1e240_0 .net "y", 0 0, L_0x2b1e340;  1 drivers
S_0x2ad41b0 .scope module, "uut" "and_gate" 3 8, 4 1 0, S_0x2ad4020;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "y";
L_0x2b1e340 .functor AND 1, v0x2b1e0a0_0, v0x2b1e170_0, C4<1>, C4<1>;
v0x2b0b170_0 .net "a", 0 0, v0x2b1e0a0_0;  1 drivers
v0x2b1de90_0 .net "b", 0 0, v0x2b1e170_0;  1 drivers
v0x2b1df50_0 .net "y", 0 0, L_0x2b1e340;  alias, 1 drivers
    .scope S_0x2ad4020;
T_0 ;
    %vpi_call/w 3 16 "$display", "A B | y" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x2b1e0a0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x2b1e170_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x2b1e0a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x2b1e170_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x2b1e0a0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x2b1e170_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x2b1e0a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x2b1e170_0, 0, 1;
    %delay 10, 0;
    %vpi_call/w 3 21 "$finish" {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x2ad4020;
T_1 ;
    %vpi_call/w 3 26 "$monitor", "At time %t: a=%b, b=%b, y=%b", $time, v0x2b1e0a0_0, v0x2b1e170_0, v0x2b1e240_0 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x2ad4020;
T_2 ;
    %vpi_call/w 3 31 "$dumpfile", "and_wave.vcd" {0 0 0};
    %vpi_call/w 3 32 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x2ad4020 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "-";
    "testbench.sv";
    "design.sv";
